<body>
  <%= render "layouts/header" %>
    <div class="login">
      <div class="logo">
        <i class="fas fa-unlock-alt"></i>
      </div>
       <%= form_for(:session, url: login_path) do |f| %>
        <%= f.label :email %>
        <%= f.email_field :email %>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <%= f.submit "Log in", class: "btn btn-primary" %>
      <% end %>
    </div>
  <%= render "layouts/footer" %>
</body>
